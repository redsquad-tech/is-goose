  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "build": "tsc --noEmit",
    "test:watch": "vitest",
    "dev": "tsx src/index.ts",
    "server:start": "tsx src/server/index.ts",
    "openapi:examples": "tsx scripts/generate-openapi-examples.ts",
