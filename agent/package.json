    "contract:dredd": "dredd requirements/GOOSE_SERVER_OPENAPI.json http://127.0.0.1:3001 --hookfiles=tests/contract/hooks.cjs --loglevel=error",
    "test:contract": "start-server-and-test \"npm run server:start\" http://127.0.0.1:3001/status \"npm run contract:dredd\"",
    "test:all": "npm run build && npm run test && npm run openapi:validate && npm run test:contract"
  },
